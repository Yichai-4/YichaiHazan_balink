public class AccountListMember {
    // Defines a map to store member - account and account list
    Map<Account, AccountList> memberSet;
    
    public static void addUpdateMember(Account newAccount, AccountList newAccountList) {
        Map<Account, AccountList> memberSet = new Map<Account, AccountList>();
        memberSet.put(newAccount, newAccountList);
        system.debug(memberSet);
        // if the insert is success return JSON with success
        // and the id of the current record
        if (true) {
            system.debug(JSON.serialize('success'));
        }
        // if the insert failed return a JSON with failed
        else {
            system.debug(JSON.serialize('failed'));
        }
    }
}
